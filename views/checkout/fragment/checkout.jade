.row(style='height:400px;overflow-y:auto;overflow-x:hidden;')
    .col-xs-12
        iframe#myIframe(name='myIframe', width='0', height='0', style='display: none;')
        form#checkout.form-horizontal(method='post',role='form',action='#{action}',target='myIframe')
            .form-group
                .row
                    .col-md-6(style="padding-right:5px;")
                        .profile-user-info.profile-user-info-striped
                            .profile-info-row
                                .profile-info-name  姓名
                                .profile-info-value
                                    span#userName
                            .profile-info-row
                                .profile-info-name  订单编号
                                .profile-info-value
                                    span#orderCode
                            .profile-info-row
                                .profile-info-name  会员编号
                                .profile-info-value
                                    span#memberCode1
                            .profile-info-row
                                .profile-info-name  游戏积分
                                .profile-info-value
                                    span#jiFen
                                    span 分
                            .profile-info-row
                                .profile-info-name  会员等级
                                .profile-info-value
                                    span#dengJi
                                    span 级
                            .profile-info-row
                                .profile-info-name  账户余额
                                .profile-info-value
                                    span#accountCash
                                    span 元
                            .profile-info-row
                                .profile-info-name  商品消费
                                .profile-info-value
                                    span#goodsTotalPrice
                                    span 元
                    .col-md-6(style="padding-left:5px;")
                        .profile-user-info.profile-user-info-striped
                            .profile-info-row
                                .profile-info-name  开始时间
                                .profile-info-value
                                    span#startTime
                                        a#startTimeBtn(style='cursor:pointer;text-decoration:underline;') 开始计时
                            .profile-info-row
                                .profile-info-name  结束时间
                                .profile-info-value
                                    span#endTime
                                        a#endTimeBtn(style='cursor:pointer;text-decoration:underline;') 结束计时
                            .profile-info-row
                                .profile-info-name  游戏时长
                                .profile-info-value
                                    span#totalTime
                                        span#totalTimeInput 请先结束计时
                            .profile-info-row
                                .profile-info-name  游戏打折
                                .profile-info-value
                                    span#zheKou ?
                                    span 折
                            .profile-info-row
                                .profile-info-name  游戏折后
                                .profile-info-value
                                    span#zheHou ?
                                    span 元
                            .profile-info-row
                                .profile-info-name  游戏余额
                                .profile-info-value
                                    span#giftCash ?
                                    span 元
                            .profile-info-row
                                .profile-info-name  游戏消费
                                .profile-info-value
                                    span#money ?
                                    span 元

            //div(class='ui-jqgrid-sortable')
                | 账户余额:
